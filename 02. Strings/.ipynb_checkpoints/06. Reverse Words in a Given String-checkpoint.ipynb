    "    \n",
    "    s = list(s.split('.'))\n",
    "    \n",
    "    res = \"\"\n",
    "    \n",
    "    for i in range(len(s)-1 ,0 ,-1):\n",
    "        res = res + s[i] + '.'\n",
    "    \n",
    "    res += s[0]\n",
    "    \n",
    "    return res\n",
    "        "
